{
    "name": "netgen/content-browser",
    "description": "Netgen Content Browser",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://www.netgenlabs.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.1",
        "ext-mbstring": "*",
        "netgen/content-browser-ui": "~0.8.0@dev",
        "symfony/symfony": "^2.8|^3.2",
        "sensio/framework-extra-bundle": "^3.0",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.1",
        "symfony/phpunit-bridge": "^3.3@rc",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0|^2.0",
        "ezsystems/ezpublish-kernel": "^6.0",
        "netgen/tagsbundle": "^2.1|^3.0",
        "sylius/product": "^1.0",
        "sylius/taxonomy": "^1.0",
        "sylius/locale": "^1.0",
        "franzl/studio": "*"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always"
    },
    "repositories": [
        { "type": "composer", "url": "https://packagist.netgen.biz" }
    ],
    "autoload": {
        "psr-4": {
            "Netgen\\ContentBrowser\\": "lib/",
            "Netgen\\Bundle\\ContentBrowserBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ],
        "psr-4": {
            "Netgen\\ContentBrowser\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\ContentBrowserBundle\\Tests\\": "tests/bundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.x-dev"
        }
    }
}
