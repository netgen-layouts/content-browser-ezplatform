{
    "name": "netgen/content-browser-bundle",
    "description": "Netgen Content Browser Bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://www.netgenlabs.com"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "symfony/symfony": "^2.8|^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "netgen/content-browser-ui-bundle": "^1.0@dev",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "symfony/phpunit-bridge": "^2.8|^3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "ezsystems/ezpublish-kernel": "^6.0",
        "netgen/tagsbundle": "^2.1",
        "sylius/core": "^0.19|^1.0"
    },
    "repositories": [
        { "type": "composer", "url": "https://packagist.netgen.biz" }
    ],
    "autoload": {
        "psr-4": {
            "Netgen\\Bundle\\ContentBrowserBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
