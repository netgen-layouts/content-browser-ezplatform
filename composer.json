{
    "name": "netgen/content-browser",
    "description": "Netgen Content Browser",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://www.netgenlabs.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "ext-mbstring": "*",
        "netgen/content-browser-ui-bundle": "^1.0@dev",
        "symfony/symfony": "^2.8|^3.2",
        "sensio/framework-extra-bundle": "^3.0",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "symfony/phpunit-bridge": "^2.8|^3.2",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "ezsystems/ezpublish-kernel": "^6.0",
        "netgen/tagsbundle": "^2.1",
        "sylius/product": "^1.0",
        "sylius/taxonomy": "^1.0",
        "sylius/locale": "^1.0",
        "franzl/studio": "*"
    },
    "repositories": [
        { "type": "composer", "url": "https://packagist.netgen.biz" }
    ],
    "autoload": {
        "psr-4": {
            "Netgen\\ContentBrowser\\": "lib/",
            "Netgen\\Bundle\\ContentBrowserBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\ContentBrowser\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\ContentBrowserBundle\\Tests\\": "tests/bundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
