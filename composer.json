{
    "name": "netgen/content-browser-ezplatform",
    "description": "Netgen Content Browser & eZ Platform integration",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "netgen/content-browser": "~0.12.0",
        "ezsystems/ezpublish-kernel": "^6.13 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/phpunit-bridge": "^4.1",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "netgen/tagsbundle": "^3.3",
        "franzl/studio": "*"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundle",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=7 --ansi tests"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\ContentBrowser\\Ez\\": "lib/",
            "Netgen\\Bundle\\ContentBrowserEzPlatformBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\ContentBrowser\\Ez\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\ContentBrowserEzPlatformBundle\\Tests\\": "tests/bundle/",
            "Netgen\\ContentBrowser\\": "vendor/netgen/content-browser/lib/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.12.x-dev"
        }
    }
}
