includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    excludes_analyse:
        - tests/application/web/bundles/

    ignoreErrors:
        # Symfony Form component
        - '#Parameter \#2 \$dataClass of class Symfony\\Component\\Form\\FormBuilder constructor expects string, null given.#'

        # JSON API Test Case
        - '#(Json)?ApiTestCase::(\w+)\(\) expects Symfony\\Component\\HttpFoundation\\Response, Symfony\\Component\\HttpFoundation\\Response\|null given.#'

